{
  "name": "ClipNote",
  "version": "1.3",
  "manifest_version": 2,
  "description": "ClipNote",
  "icons": {
      "142": "icons/142.png"
  },
  "browser_action": {
    "default_icon": "icons/142.png",
    "default_title": "ClipNote",
    "default_popup": "popup/popup.html"
  },
  "content_scripts": [
      {
          "matches": [
            "<all_urls>"
          ],
          "js": ["content-scripts/clipnote.js"]
      }
  ]
}