{
  "name": "ClipNote",
  "version": "1.3",
  "manifest_version": 3,
  "description": "ClipNote",
  "icons": {
      "128": "icons/128.png"
  },
  "action": {
      "default_popup": "popup/popup.html"
  },
  "content_scripts": [
      {
          "matches": [
            "<all_urls>"
          ],
          "js": ["content-scripts/clipnote.js"]
      }
  ]
}
